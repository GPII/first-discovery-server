{
  "name": "first-discovery-server",
  "version": "0.1.0-SNAPSHOT",
  "description": "A server side implementation to serve a First Discovery Editor instance and provide a means for storing preferences to the GPII Preferences server.",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  },
  "scripts": {
      "dedupe-infusion": "node -e \"require('dedupe-infusion')()\"",
      "fullinstall": "npm install && npm run dedupe-infusion",
      "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GPII/first-discovery-server.git"
  },
  "keywords": [
    "preferences",
    "first discovery",
    "server",
    "javascript",
    "gpii",
    "fluid",
    "infusion"
  ],
  "author": "GPII",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://issues.gpii.net"
  },
  "homepage": "http://gpii.net",
  "dependencies": {
    "first-discovery": "git+https://github.com/GPII/first-discovery.git",
    "gpii-express": "git+https://github.com/GPII/gpii-express.git",
    "infusion": "git://github.com/amb26/infusion.git#539c8dc6bdd2170944c0a4c58da52dd37fb098c0",
    "kettle":  "git://github.com/amb26/kettle.git#c7207e93518b3c61cb6e923f22b1e26f2c365f64",
     "dedupe-infusion": "1.0.0"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "~0.9.0",
    "grunt-jsonlint": "1.0.4",
    "node-jqunit": "1.1.0"
  }
}
