{
    "type": "firstDiscovery.server.environment.config",
    "options": {
        "gradeNames": ["gpii.firstDiscovery.server.configurator"],
        "port": "{environment}.vars.FIRST_DISCOVERY_SERVER_TCP_PORT",
        "preferencesConfig": {
            "securityServer": {
                "port": "{environment}.vars.GPII_OAUTH2_TCP_PORT",
                "hostname": "{environment}.vars.GPII_OAUTH2_HOST_NAME",
                "paths": {
                    "token": "{environment}.vars.GPII_OAUTH2_ACCESS_TOKEN_PATH",
                    "preferences": "{environment}.vars.GPII_OAUTH2_ADD_PREFERENCES_PATH"
                }
            },
            "authentication": {
                "grant_type": "{environment}.vars.GPII_OAUTH2_AUTH_GRANT_TYPE",
                "scope": "{environment}.vars.GPII_OAUTH2_AUTH_SCOPE",
                "client_id": "{environment}.vars.GPII_OAUTH2_AUTH_CLIENT_ID",
                "client_secret": "{environment}.vars.GPII_OAUTH2_AUTH_CLIENT_SECRET"
            }
        }
    },
    "require": ["../js/firstDiscoveryServer.js"]
}
